@using System.Data
@model DataTable

@{
    ViewBag.Title = "Page2";
}

<table>
    <tr>
    @foreach(DataColumn dataColumn in Model.Columns){
         <th style="border:1px solid #000;">@dataColumn.ColumnName</th>   
    }
    </tr>
    @foreach (DataRow studyProgramRow in Model.Rows)
    {
        <tr>
            @for (int i = 0; i < Model.Columns.Count; ++i)
            {
                <td style="border:1px solid #000;">@studyProgramRow[i]</td>
            }
        </tr>
    }
</table>
@Html.ActionLink("Link back to home", "Index", "Home")

<!-- Position of the section is irrelevant -->
@section PageHeader {
   <h2>Page2 - List of all study programs</h2>
}